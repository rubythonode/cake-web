{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "exclude": [
      "dist",
      "node_modules"
    ]
  },
  "rules": {
    "typedef": [
      true,
      "call-signature",
      "object-destructuring",
      "variable-declaration"
    ],
    "no-boolean-literal-compare": false
  }
}
